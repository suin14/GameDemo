;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Demo"
run/main_scene="res://scenes/UI/title_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Game="*res://scenes/globals/game.tscn"
SoundManager="*res://scenes/globals/sound_manager.tscn"
PlayerStatus="*res://scenes/globals/player_status.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
PlayerAction="*res://scenes/globals/player_action.tscn"

[dialogic]

directories/dch_directory={
"UnknownNpc": "res://assets/character/UnknownNpc.dch",
"character/unit_test_character": "res://assets/character/unit_test_character.dch",
"player": "res://assets/character/player/player.dch"
}
directories/dtl_directory={
"Bed_sleep": "res://content/request/Bed_sleep.dtl",
"story-00": "res://content/dialogue/story-00.dtl",
"test_dialogue": "res://content/test_dialogue.dtl"
}
text/letter_speed=0.03
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
translation/enabled=false
translation/original_locale="zh_CN"
translation/file_mode=1
translation/translation_folder=""
translation/save_mode=0
translation/add_separator=false
text/autopauses={}
variables={}
layout/style_list=["res://assets/ui/dialogue/dialogue_style.tres", "res://assets/ui/dialogue/default_style.tres"]
layout/default_style="res://assets/ui/dialogue/default_style.tres"
glossary/default_case_sensitive=true
layout/end_behaviour=0
text/autocolor_names=false
text/input_action="dialogic_default_action"
timer/process_in_physics=false
save/autosave=true
text/autoadvance_enabled=false
animations/join_default="res://addons/dialogic/Modules/Character/DefaultAnimations/zoom_in.gd"

[display]

window/size/viewport_width=512
